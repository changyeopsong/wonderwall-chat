{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "plugins": ["locale-check", "react-hooks"],
    "rules": {
        "locale-check/no-key-locale": 2,
        "react-hooks/exhaustive-deps": "off"
    }
}
